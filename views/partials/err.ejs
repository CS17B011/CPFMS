<% if(flash) { %>
    <% var len = flash.length,message; %>
    <% for(var i=0; i < len; ++i ) { %>
        <% console.log(i,flash); %>
        <% message = flash.shift(); %>
        <div class="alert alert-<%= message.type %>">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <strong><%= message.message %></strong>
        </div>
    <% } %>
<% } %>